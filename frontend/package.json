{
  "main": "electron/main.js",
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "start": "electron .",
    "electron:build": "electron-builder"
  },
  "build": {
    "appId": "com.seuapp.id",
    "productName": "MeuApp",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "electron/assets"
    },
    "win": {
      "target": "nsis",
      "icon": "electron/assets/icon.ico"
    }
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.3",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^2.2.12"
  }
}
